{
  "name": "espressobyte/firemobile-php",
  "description": "Firemobile API Client for PHP",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Mior Muhammad Zaki",
      "email": "crynobone@katsana.com"
    },
    {
      "name": "Tajul Asri",
      "email": "mtajulasri@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Firemobile\\": "src/"
    }
  },
  "require": {
    "php": "^7.2 || ^8.0",
    "laravie/codex": "^5.1",
    "laravie/codex-common": "^1.4",
    "php-http/multipart-stream-builder": "^1.0"
  },
  "require-dev": {
    "guzzlehttp/guzzle": "^7.4",
    "illuminate/notifications": "^5.8 || ^6.0 || ^7.0 || ^8.0",
    "illuminate/support": "^5.8 || ^6.0 || ^7.0 || ^8.0",
    "laravie/parser": "^2.0",
    "mockery/mockery": "^1.3",
    "php-http/guzzle7-adapter": "^0.1.1",
    "phpunit/phpunit": "^7.5 || ^8.4 || ^9.0"
  },
  "suggest": {
    
  },
  "scripts": {
    "test": [
      "./vendor/bin/phpunit"
    ]
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Fireobile\\Laravel\\SilverstreetServiceProvider",
        "Firemobile\\Laravel\\Notification\\ServiceProvider"
      ]
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
